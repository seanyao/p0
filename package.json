{
  "name": "travel-route-map-vue",
  "version": "1.0.0",
  "description": "一个极简美观的旅游路线图生成工具 - Vue 3 + TypeScript版本",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.vue",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.vue --fix",
    "format": "prettier --write .",
    "type-check": "vue-tsc --noEmit"
  },
  "keywords": [
    "travel",
    "route",
    "map",
    "visualization",
    "tourism",
    "planning",
    "vue3",
    "typescript"
  ],
  "author": "Travel Route Map Team",
  "license": "MIT",
  "dependencies": {
    "vue": "^3.4.0",
    "vue-router": "^4.2.0",
    "pinia": "^2.1.0",
    "axios": "^1.6.0",
    "@vueuse/core": "^10.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^3.0.0",
    "typescript": "~5.3.0",
    "vite": "^4.0.0",
    "vue-tsc": "^1.8.0",
    "vitest": "^0.34.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/travel-route-map/travel-route-map.git"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}