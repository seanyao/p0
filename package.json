{
  "name": "travel-route-map",
  "version": "1.0.0",
  "description": "一个极简美观的旅游路线图生成工具 - Vue 3 + TypeScript版本",
  "main": "index.html",
  "scripts": {
    "dev": "vite --config vite.config.ts",
    "build": "vue-tsc && vite build --config vite.config.ts",
    "preview": "vite preview --config vite.config.ts",
    "test": "vitest",
    "lint": "eslint . --ext .js,.ts,.vue",
    "lint:fix": "eslint . --ext .js,.ts,.vue --fix",
    "format": "prettier --write .",
    "type-check": "vue-tsc --noEmit"
  },
  "keywords": [
    "travel",
    "route",
    "map",
    "visualization",
    "tourism",
    "planning",
    "vue3",
    "typescript"
  ],
  "author": "Travel Route Map Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^3.0.0",
    "terser": "^5.44.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "vitest": "^0.34.0",
    "vue-tsc": "^1.8.0"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.0",
    "axios": "^1.6.0",
    "pinia": "^2.1.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/travel-route-map/travel-route-map.git"
  },
  "bugs": {
    "url": "https://github.com/travel-route-map/travel-route-map/issues"
  },
  "homepage": "https://github.com/travel-route-map/travel-route-map#readme"
}
